SELECT * FROM `students` WHERE `date_of_birth` LIKE "1990%"
SELECT * FROM `courses` WHERE`cfu` > "10"
SELECT * FROM `students` WHERE Year(`date_of_birth`) < "1992"
SELECT * FROM `courses` WHERE `year` LIKE "1" AND `period` LIKE "I SEMESTRE"
SELECT * FROM `exams` WHERE `date` LIKE "20/06/2020" AND HOUR(`hour`) > "14"
SELECT * FROM `degrees` WHERE `level` LIKE "magistrale"
SELECT * FROM `departments` 
SELECT * FROM `teachers` WHERE `phone` IS NULL OR ""
